{
    "injectionSelector": "L:comment.block.documentation",
    "patterns": [
        {
            "include": "#jsdocbody"
        }
    ],
    "repository": {
        "jsdocbody": {
            "begin": "(?<=/\\*\\*)([^*]|\\*(?!/))*$",
            "while": "(^|\\G)\\s*\\*(?!/)(?=([^*]|[*](?!/))*$)",
            "patterns": [
                {
                    "include": "source.tg#docblock"
                }
            ]
        }
    },
    "scopeName": "documentation.injection.tg"
}
